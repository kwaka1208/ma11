[
	// アクションが入っていて、テキストが入っていない場合は、アクションのみ行う。
	{
		"Action": "Act-positive-2",
		"Txt": ""
	},

	// アクションが空で、テキストが入っている場合は、発話のみ行う。
	{
		"Action": "",
		"Txt": "こんにちは"
	},

	//両方とも入っている場合は、発話->アクションの順で処理する。
	{
		"Action": "Act-positive-2",
		"Txt": "こんにちは"
	},
	
	//両方とも空の場合は無視する。
	{
		"Action": "",
		"Txt": ""
	},
]
